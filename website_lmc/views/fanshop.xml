<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="fanshop_lmc" model="ir.ui.view">
            <field name="name">Fanshop</field>
            <field name="type">qweb</field>
            <field name="key">website_lmc.fanshop_lmc</field>
            <field name="arch" type="xml">
